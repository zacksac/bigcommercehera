{
  "components/products/options/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\" data-product-option-id=\"{{id}}\" role=\"radiogroup\" aria-labelledby=\"radio-group-label\">\n    <label class=\"form-label form-label--inlineSmall\" id=\"radio-group-label\">\n        {{ this.display_name }}\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    {{#unless required}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_radio_{{../id}}_none\"\n            name=\"attribute[{{../id}}]\"\n            value=\"\"\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\n            data-title=\"{{this.label}}\"\n            data-index=\"{{@index}}\"\n        >\n        <label class=\"form-label\" for=\"attribute_radio_{{../id}}_none\">{{lang 'products.none'}}</label>\n    {{/unless}}\n\n    <div class=\"form-radio-all\">\n        {{#each this.values}}\n            <div class=\"form-radio-{{@index}}\">\n                <input data-filter=\".filter-{{camelcase this.label}}\"\n                    class=\"form-radio\"\n                    type=\"radio\"\n                    id=\"attribute_radio_{{../id}}_{{id}}\"\n                    name=\"attribute[{{../id}}]\"\n                    value=\"{{id}}\"\n                    {{#if selected}}\n                        checked\n                        data-default\n                    {{/if}}\n                    {{#if ../required}}required{{/if}}\n                    data-title=\"{{this.label}}\"\n                    data-index=\"{{@index}}\"\n                >\n                <label data-product-attribute-value=\"{{id}}\" class=\"form-label form-radio-child\" for=\"attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\n            </div>\n        {{/each}}\n    </div>\n</div>\n",
  "components/common/requireness-msg": "<small>\n    {{#if required}}*{{/if}}\n</small>\n"
}
