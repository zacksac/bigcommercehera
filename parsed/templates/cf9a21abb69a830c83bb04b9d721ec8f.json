{
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\" data-header-logo-link>\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            {{#if theme_settings.logo_transparent}}\n                <img class=\"header-logo-image-unknown-size header-logo-transparent\" src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.logo_transparent}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            {{/if}}\n            <img class=\"header-logo-image-unknown-size{{#unless theme_settings.logo_transparent}} no-logo-transparent{{/unless}}\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                {{#if theme_settings.logo_transparent}}\n                    <img class=\"header-logo-image header-logo-transparent\" src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.logo_transparent}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n                {{/if}}\n                <img class=\"header-logo-image{{#unless theme_settings.logo_transparent}} no-logo-transparent{{/unless}}\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"
}
