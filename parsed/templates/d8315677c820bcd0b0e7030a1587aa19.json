{
  "components/category/show-more": "{{#if category}}\n    {{> components/faceted-search/show-more-facets category}}\n{{/if}}\n",
  "components/faceted-search/show-more-facets": "<div class=\"modal-header\">\n    <h2 class=\"modal-header-title facet-quick-heading\">{{all_facets}}</h2>\n</div>\n<div class=\"modal-body\">\n    <div class=\"form-field\" id=\"facetedSearch-filterItems\">\n        <input class=\"form-input\" type=\"search\" placeholder=\"{{lang 'forms.search.search'}}\">\n    </div>\n    <ul class=\"navList facetedSearch-optionColumns\">\n        {{#each facets}}\n            {{#if facet '===' ../all_facets}}\n                {{#each items}}\n                    <li class=\"navList-item\">\n                        <a\n                        href=\"{{url}}\"\n                        class=\"navList-action{{#if selected}} is-active{{/if}}\"\n                        rel=\"nofollow\"\n                        data-id=\"{{id}}\"\n                        data-faceted-search-facet\n                        >\n                            <span class=\"text\">\n                                {{{title}}}\n                                {{#if ../show_product_counts}}\n                                    {{#if count}}\n                                        <span>({{count}})</span>\n                                    {{/if}}\n                                {{/if}}\n                            </span>\n                        </a>\n                    </li>\n                {{/each}}\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
}
