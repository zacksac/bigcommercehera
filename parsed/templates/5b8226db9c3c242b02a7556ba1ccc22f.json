{
  "components/products/options/input-checkbox": "<div class=\"form-field\" data-product-attribute=\"input-checkbox\">\n    <label class=\"form-label form-label--inlineSmall\">\n        {{display_name}}\n        {{> components/common/requireness-msg}}\n    </label>\n    <input type=\"hidden\" name=\"attribute[{{id}}]\" value=\"{{noValue}}\" />\n    <div class=\"form-checkbox-{{id}}\">\n        <input\n            class=\"form-checkbox\"\n            type=\"checkbox\"\n            name=\"attribute[{{id}}]\"\n            id=\"attribute-check-{{id}}\"\n            value=\"{{value}}\"\n            {{#if checked}}\n                checked\n                data-default\n            {{/if}}\n            {{#if required}}required{{/if}}>\n\n        <label class=\"form-label form-checkbox-child {{class}}\" for=\"attribute-check-{{id}}\">{{label}}</label>\n    </div>\n</div>\n",
  "components/common/requireness-msg": "<small>\n    {{#if required}}*{{/if}}\n</small>\n"
}
