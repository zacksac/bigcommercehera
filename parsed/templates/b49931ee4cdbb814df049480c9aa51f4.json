{
  "components/products/reviews": "<div class=\"productView-reviews-wrapper productReviews\">\n    {{#if reviews.total}}\n        <div class=\"productReview-total\">\n            <span>\n                {{lang 'products.reviews.link_to_review' total=reviews.total}}\n            </span>\n        </div>\n        <ul class=\"productReviews-list\" id=\"productReviews-list\">\n            {{#each reviews.list}}\n                <li class=\"productReview\">\n                    <article>\n                        <header>\n                            <h5 class=\"productReview-title\">\n                                <span class=\"productReview-title-text\">{{{ sanitize title }}}</span>\n                                <span class=\"productReview-rating rating--small\">\n                                    {{> components/products/ratings rating=rating}}\n                                    <span class=\"productReview-ratingNumber\">{{ rating }}</span>\n                                </span>\n                            </h5>\n                            {{#if name}}\n                                <meta content=\"{{name}}\">\n                                <p class=\"productReview-author\">\n                                    {{lang 'products.reviews.post_on_by' date=date name=name }}\n                                </p>\n                            {{else}}\n                                <p class=\"productReview-author\">\n                                    {{lang 'products.reviews.post_on_by' date=date name=(lang 'products.reviews.anonymous_poster')}}\n                                </p>\n                            {{/if}}\n                        </header>\n                        <p class=\"productReview-body\">{{{ sanitize text }}}</p>\n                    </article>\n                </li>\n            {{/each}}\n        </ul>\n        {{#or pagination.reviews.next pagination.reviews.previous}}\n            {{> components/common/paginator pagination.reviews}}\n        {{/or}}\n        <div class=\"button-createReview\">\n            <a class=\"button button--primary\" href=\"#\" data-reveal-id=\"modal-review-form\">{{lang 'products.reviews.write_a_review'}}</a>\n        </div>\n    {{else}}\n        <div class=\"alertBox alertBox--warning\">\n            <div class=\"alertBox-column alertBox-icon\">\n                <svg class=\"icon\"><use xlink:href=\"#icon-info\"></use></svg>\n            </div>\n            <p class=\"alertBox-column alertBox-message\">\n                {{lang 'products.reviews.write_a_review_text'}}\n            </p>\n        </div>\n        <div class=\"button-createReview\">\n            <a class=\"button button--primary\" href=\"#\" data-reveal-id=\"modal-review-form\">{{lang 'products.reviews.write_a_review'}}</a>\n        </div>\n    {{/if}}\n</div>\n",
  "components/products/ratings": "<span role=\"img\"\n    {{#if rating_target}}\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target=rating_target current_rating=rating max_rating=5}}\"\n    {{else}}\n      aria-label=\"{{lang 'products.reviews.rating_aria_label' rating_target='Product' current_rating=rating max_rating=5}}\"\n    {{/if}}\n>\n    {{#for 1 5}}\n        {{#if ../this.rating '>=' $index}}\n            <span class=\"icon icon--ratingFull\">\n                <svg>\n                    <use xlink:href=\"#icon-star\" />\n                </svg>\n            </span>\n        {{else}}\n            <span class=\"icon icon--ratingEmpty\">\n                <svg>\n                    <use xlink:href=\"#icon-star\" />\n                </svg>\n            </span>\n        {{/if}}\n    {{/for}}\n</span>\n",
  "components/common/paginator": "<nav class=\"pagination{{#if theme_settings.halo-product-show-more}} pagination-2{{/if}}\" aria-label=\"pagination\">\n    {{#if page_type '==' 'category'}}\n        {{#if total}}\n            {{#if start}}\n                {{#if end}}\n                    {{#if end '<' total}}\n                        <div class=\"pagination-info\">Items <span class=\"start\">{{start}}</span> to <span class=\"end\">{{end}}</span> of <span class=\"total\">{{total}}</span> total</div>\n                        <div class=\"pagination-total-progress\">\n                            <span class=\"pagination-total-item\"></span>\n                        </div>\n                    {{else}}\n                        <div class=\"pagination-info\">Items <span class=\"start\">{{start}}</span> to <span class=\"end\">{{total}}</span> of <span class=\"total\">{{total}}</span> total</div>\n                        <div class=\"pagination-total-progress\">\n                            <span class=\"pagination-total-item\"></span>\n                        </div>\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n        {{/if}}\n        {{#if theme_settings.halo-product-show-more}}\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\n                {{#if next}}\n                    <a class=\"button button--primary\" href=\"#\">\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\n                    </a>\n                {{else}}\n                    <a class=\"button button--primary disable\" href=\"#\">\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\n                    </a>\n                {{/if}}\n            </div> \n        {{/if}}\n    {{/if}}\n    {{#if page_type '==' 'brand'}}\n        {{#if links.length}}\n            {{#each shop_by_brand}}\n                {{#if name '==' ../brand.name}}\n                    <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{count}}</span> total</div>\n                    <div class=\"pagination-total-progress\">\n                        <span class=\"pagination-total-item\"></span>\n                    </div>\n                {{/if}}\n            {{/each}}\n        {{/if}}\n        {{#if theme_settings.halo-product-show-more}}\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\n                {{#if next}}\n                    <a class=\"button button--primary\" href=\"#\">\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\n                    </a>\n                {{else}}\n                    <a class=\"button button--primary disable\" href=\"#\">\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\n                    </a>\n                {{/if}}\n            </div> \n        {{/if}}\n    {{/if}}\n    {{#if page_type '==' 'search'}}\n        <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{total}}</span> total</div>\n        <div class=\"pagination-total-progress\">\n            <span class=\"pagination-total-item\"></span>\n        </div>\n        {{#if theme_settings.halo-product-show-more}}\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\n                {{#if next}}\n                    <a class=\"button button--primary\" href=\"#\">\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\n                    </a>\n                {{else}}\n                    <a class=\"button button--primary disable\" href=\"#\">\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\n                    </a>\n                {{/if}}\n            </div> \n        {{/if}}\n    {{/if}}\n    <ul class=\"pagination-list\">\n        {{#if previous}}\n            <li class=\"pagination-item pagination-item--previous\">\n                <a class=\"pagination-link\"\n                   href=\"{{previous}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.previous'}}\"\n                >\n                    <svg class=\"icon\">\n                        <use xlink:href=\"#icon-chevron-left\"></use>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n\n        {{#each links}}\n            <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\n                <a class=\"pagination-link\"\n                   href=\"{{this.url}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   {{#if this.number '==' ../current}}aria-current=\"page\"{{/if}}\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\n                >\n                    {{this.number}}\n                </a>\n            </li>\n        {{/each}}\n\n        {{#if next}}\n            <li class=\"pagination-item pagination-item--next\">\n                <a class=\"pagination-link\"\n                   href=\"{{next}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.next'}}\"\n                >\n                    <svg class=\"icon\">\n                        <use xlink:href=\"#icon-chevron-right\"></use>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n"
}
