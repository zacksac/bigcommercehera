{
  "components/common/navigation-menu": "<nav class=\"navPages\">\n    <ul class=\"navPages-list{{#if theme_settings.haloMegamenu}} navPages-list-megamenu{{/if}}{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\n        {{#each categories}}\n            {{#if name '!=' ../theme_settings.halo_menu_tab_title_2}}\n                <li class=\"navPages-item{{#if children}} has-dropdown{{/if}}\">\n                    {{#if ../theme_settings.navigation_design '===' 'simple'}}\n                        {{> components/common/navigation-list}}\n                    {{/if}}\n                    {{#if ../theme_settings.navigation_design '===' 'alternate'}}\n                        {{> components/common/navigation-list-alternate}}\n                    {{/if}}\n                </li>\n            {{/if}}\n        {{/each}}\n        {{#unless theme_settings.hide_content_navigation}}\n            {{#each pages}}\n                <li class=\"navPages-item navPages-item-page{{#if children}} has-dropdown{{/if}}\">\n                    {{> components/common/navigation-list-alternate}}\n                </li>\n            {{/each}}\n        {{/unless}}\n    </ul>\n</nav>\n",
  "components/common/navigation-list": "{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   data-collapsible=\"navPages-{{id}}\"\n>\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\">\n            <a class=\"navPage-subMenu-action navPages-action\"\n               href=\"{{url}}\"\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n            >\n                {{lang 'category.view_all.name' category=name}}\n            </a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                        <span class=\"collapsible-icon-wrapper\"\n                            data-collapsible=\"navPages-{{id}}\"\n                            data-collapsible-disabled-breakpoint=\"medium\"\n                            data-collapsible-disabled-state=\"open\"\n                            data-collapsible-enabled-state=\"closed\"\n                        >\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\n                            </i>\n                        </span>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                               href=\"{{url}}\"\n                               aria-label=\"{{name}}\"\n                            >\n                                {{name}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n",
  "components/common/navigation-list-alternate": "{{#if children}}\n    <p class=\"navPages-action navPages-action-lv1 navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\" data-label=\"{{url}}\" data-index=\"{{@index}}\">\n        <a class=\"text {{name}}\" href=\"{{url}}\" aria-label=\"{{name}}\">\n          <span>{{name}}</span>\n        </a>\n        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n        </span>\n    </p>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action navPages-action-lv1 no-subMenu navPages-action-end{{#if is_active}} activePage{{/if}}\" data-label=\"{{url}}\" href=\"{{url}}\" aria-label=\"{{name}}\" data-index=\"{{@index}}\">\n      <span class=\"text\">{{name}}</span>\n    </a>\n{{/if}}\n\n\n",
  "components/common/navigation-dropdown": "<div class=\"navPage-subMenu navPage-subMenu-horizontal level-2\" id=\"navPages-{{id}}\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action navPages-action-lv1 navPages-action-depth-max has-subMenu\">\n                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                    <svg class=\"icon\"><use xlink:href=\"#icon-long-arrow-left\" /></svg>\n                </span>\n                <a class=\"text\" href=\"{{url}}\"><span>{{name}}</span></a>\n            </p>\n        </li>\n        <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-lv1 no-subMenu\" href=\"{{url}}\">\n                <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n            </a>\n        </li>\n        {{#each children}}\n            {{#if children}}\n                <li class=\"navPage-subMenu-item-child has-dropdown\">\n                    <p class=\"navPage-subMenu-action navPages-action navPages-action-lv2 navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" data-navId=\"navPages-{{id}}\">\n                        <a class=\"text\" href=\"{{url}}\"><span>{{name}}</span></a>\n                        <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                            <svg class=\"icon\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                        </span>\n                    </p>\n                    <div class=\"navPage-subMenu navPage-subMenu-horizontal level-3\" aria-hidden=\"true\" tabindex=\"-1\">\n                        <ul class=\"navPage-subMenu-list\">\n                            <li class=\"navPage-subMenu-item-parent navPage-subMenu-item-child navPage-subMenu-title\">\n                                <p class=\"navPage-subMenu-action navPages-action navPages-action-lv2\">\n                                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                                        <svg class=\"icon\"><use xlink:href=\"#icon-long-arrow-left\" /></svg>\n                                    </span>\n                                    <a class=\"text\" href=\"{{url}}\"><span>{{name}}</span></a>\n                                </p>\n                            </li>\n                            <li class=\"navPage-subMenu-item-child navPages-action-end navPage-subMenu-all\">\n                                <a class=\"navPage-subMenu-action navPages-action navPages-action-lv2 no-subMenu\" href=\"{{url}}\">\n                                    <span class=\"text\">{{lang 'category.view_all.name' category=name}}</span>\n                                </a>\n                            </li>\n                            {{#each children}}\n                                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                                    <a class=\"navPage-subMenu-action navPages-action navPages-action-lv3 no-subMenu level-3\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                                </li>\n                            {{/each}}\n                        </ul>\n                    </div>\n                </li>\n            {{else}}\n                <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-lv2 no-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\"><span class=\"text\">{{name}}</span></a>\n                </li>\n            {{/if}}\n        {{/each}}\n    </ul>\n</div>\n"
}
