{
  "components/products/options/set-rectangle": "<div class=\"form-field\" data-product-attribute=\"set-rectangle\" data-product-option-id=\"{{id}}\" role=\"radiogroup\" aria-labelledby=\"rectangle-group-label\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" id=\"rectangle-group-label\">\n        {{this.display_name}}\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    {{#unless required}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_rectangle__{{../id}}_none\"\n            name=\"attribute[{{../id}}]\"\n            value=\"\"\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\n            data-title=\"{{this.label}}\"\n            data-index=\"{{@index}}\"\n        >\n        <label class=\"form-option\" for=\"attribute_rectangle__{{../id}}_none\">\n            <span class=\"form-option-variant\">{{lang 'products.none'}}</span>\n        </label>\n    {{/unless}}\n\n    <div class=\"option-value-size\">\n        {{#each this.values}}\n            <input data-filter=\".filter-{{camelcase this.label}}\"\n                class=\"form-radio\"\n                type=\"radio\"\n                id=\"attribute_rectangle__{{../id}}_{{id}}\"\n                name=\"attribute[{{../id}}]\"\n                value=\"{{id}}\"\n                {{#if selected}}\n                    checked\n                    data-default\n                {{/if}}\n                {{#if ../required}}required{{/if}}\n                data-title=\"{{this.label}}\"\n                data-index=\"{{@index}}\"\n            >\n            <label class=\"form-option form-option-rectangle\" for=\"attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\n                <span class=\"form-option-variant\"><span>{{this.label}}</span></span>\n            </label>\n        {{/each}}\n    </div>\n</div>\n",
  "components/common/requireness-msg": "<small>\n    {{#if required}}*{{/if}}\n</small>\n"
}
